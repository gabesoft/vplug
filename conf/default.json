{
    "app": {
        "port": 8007
    },
    "github": {
        "api": {
            "search": "https://api.github.com/search/repositories"
        }
    },
    "keywords": {
        "api": {
            "delete": "http://localhost:8006/vplugkeywords",
            "save": "http://localhost:8006/vplugkeywords"
        },
        "chunklen": 5000
    },
    "redis": {
        "expire": {
            "unit": "days",
            "value": 30
        },
        "host": "localhost",
        "port": 6379,
        "prefix": "vplug-cache"
    },
    "tasks": {
        "interval": {
            "unit": "minutes",
            "value": 30
        }
    },
    "userAgent": "Ayne-Finder/0.0.1",
    "vplugs": {
        "api": {
            "create": "http://localhost:8006/vplugs",
            "fetch": "http://localhost:8006/vplugs"
        }
    }
}
